package com.project.web.bean;

import java.sql.Date;

/**
 * 日志实体类
 * @author TE
 *
 */
public class LogBean {
	/**
	 * 日志id
	 */
   private int id;
   /**
    * 日志内容
    */
   private String log;
   /**
    * 日志时间
    */
   private Date date;
@Override
public String toString() {
	return "LogBean [id=" + id + ", log=" + log + ", date=" + date + "]";
}
public LogBean() {
	super();
	// TODO Auto-generated constructor stub
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getLog() {
	return log;
}
public void setLog(String log) {
	this.log = log;
}
public Date getDate() {
	return date;
}
public void setDate(Date date) {
	this.date = date;
}
public LogBean(String log, Date date) {
	super();
	this.log = log;
	this.date = date;
}

}