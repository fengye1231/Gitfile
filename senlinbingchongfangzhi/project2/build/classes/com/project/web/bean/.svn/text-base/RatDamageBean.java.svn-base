package com.project.web.bean;

/**
 * 鼠害实体类
 * 
 * @author TE
 *
 */
public class RatDamageBean {
	/**
	 * 鼠害id
	 */
	private int id;
	/**
	 * 鼠害名称
	 */
	private String name;
	/**
	 * 鼠害食物
	 */
	private String food;
	/**
	 * 鼠害繁殖
	 */
	private String duction;
	/**
	 * 鼠害天敌
	 */
	private String enemy;
	/**
	 * 鼠害图片
	 */
	private String imgPath;
	/**
	 * 防治措施
	 */
	private String prevention;
	/**
	 * 主要危害
	 */
	private String harm;
	
	
	public RatDamageBean() {
		super();
		// TODO Auto-generated constructor stub
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getFood() {
		return food;
	}
	public void setFood(String food) {
		this.food = food;
	}
	public String getDuction() {
		return duction;
	}
	public void setDuction(String duction) {
		this.duction = duction;
	}
	public String getEnemy() {
		return enemy;
	}
	public void setEnemy(String enemy) {
		this.enemy = enemy;
	}
	public String getImgPath() {
		return imgPath;
	}
	public void setImgPath(String imgPath) {
		this.imgPath = imgPath;
	}
	public String getPrevention() {
		return prevention;
	}
	public void setPrevention(String prevention) {
		this.prevention = prevention;
	}
	public String getHarm() {
		return harm;
	}
	public void setHarm(String harm) {
		this.harm = harm;
	}
	@Override
	public String toString() {
		return "RatDamageBean [id=" + id + ", name=" + name + ", food=" + food + ", duction=" + duction + ", enemy="
				+ enemy + ", imgPath=" + imgPath + ", prevention=" + prevention + ", harm=" + harm + "]";
	}
	public RatDamageBean(String name, String food, String duction, String enemy, String imgPath, String prevention,
			String harm) {
		super();
		this.name = name;
		this.food = food;
		this.duction = duction;
		this.enemy = enemy;
		this.imgPath = imgPath;
		this.prevention = prevention;
		this.harm = harm;
	}

}