<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ysj.core.dao.TitleMapper" >
  <resultMap id="Distribution" type="com.ysj.core.po.Distribution" >
  
    <id column="s_id" property="sId" jdbcType="VARCHAR" />
    <result column="s_name" property="sName" jdbcType="VARCHAR" />
    <result column="t_id" property="tId" jdbcType="VARCHAR" />
    <result column="t_name" property="tName" jdbcType="VARCHAR" />

    <association column="t_id"  property="tId" javaType="Teacher"
        select="com.ysj.core.dao.TeacherMapper.selectByPrimaryKey" ></association>
        
    <association column="s_id"  property="sId" javaType="Student"
        select="com.ysj.core.dao.StudentMapper.selectByPrimaryKey" ></association>
        
  </resultMap>
  <insert id="insert" parameterType="com.ysj.core.po.Distribution">
    insert into distribution (s_id, s_name, t_id, 
    t_name
      )
    values (#{sId,jdbcType=VARCHAR}, #{sName,jdbcType=VARCHAR}, #{tId,jdbcType=VARCHAR}, #{tName,jdbcType=VARCHAR}
     )
  </insert>


  <sql id="Base_Column_List" >
    s_id, s_name, t_id, tname
  </sql>




  <select id="selectByPrimaryKey" resultMap="Distribution" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from student
    where s_id = #{sId,jdbcType=VARCHAR}
  </select>



</mapper>