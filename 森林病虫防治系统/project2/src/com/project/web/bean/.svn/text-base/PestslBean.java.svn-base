package com.project.web.bean;
/**
 * 虫害实体类
 * @author TE
 *
 */
public class PestslBean {
	/**
	 * 虫害id
	 */
   private int id;
	/**
	 * 虫害名称
	 */
   private String name;
	/**
	 * 宿主
	 */
   private String host;
	/**
	 * 繁殖
	 */
   private String duction;
	/**
	 * 天敌
	 */
   private String enemy;
	/**
	 * 幼虫图片
	 */
   private String larvaImg;
	/**
	 * 成虫图片
	 */
   private String adultImg;
	/**
	 * 防治措施
	 */
   private String prevention;
	/**
	 * 主要危害
	 */
   private String harm;
	@Override
	public String toString() {
		return "PestslBean [id=" + id + ", name=" + name + ", host=" + host + ", duction=" + duction + ", enemy="
				+ enemy + ", larvaImg=" + larvaImg + ", adultImg=" + adultImg + ", prevention=" + prevention + ", harm="
				+ harm + "]";
	}
	public PestslBean() {
		super();
		// TODO Auto-generated constructor stub
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getHost() {
		return host;
	}
	public void setHost(String host) {
		this.host = host;
	}
	public String getDuction() {
		return duction;
	}
	public void setDuction(String duction) {
		this.duction = duction;
	}
	public String getEnemy() {
		return enemy;
	}
	public void setEnemy(String enemy) {
		this.enemy = enemy;
	}
	public String getLarvaImg() {
		return larvaImg;
	}
	public void setLarvaImg(String larvaImg) {
		this.larvaImg = larvaImg;
	}
	public String getAdultImg() {
		return adultImg;
	}
	public void setAdultImg(String adultImg) {
		this.adultImg = adultImg;
	}
	public String getPrevention() {
		return prevention;
	}
	public void setPrevention(String prevention) {
		this.prevention = prevention;
	}
	public String getHarm() {
		return harm;
	}
	public void setHarm(String harm) {
		this.harm = harm;
	}
	public PestslBean(String name, String host, String duction, String enemy, String larvaImg, String adultImg,
			String prevention, String harm) {
		super();
		this.name = name;
		this.host = host;
		this.duction = duction;
		this.enemy = enemy;
		this.larvaImg = larvaImg;
		this.adultImg = adultImg;
		this.prevention = prevention;
		this.harm = harm;
	}

}